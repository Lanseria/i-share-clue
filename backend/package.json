{
  "name": "i-share-clue-nestjs",
  "version": "0.0.1",
  "description": "Based Nestjs i-share-clue backend",
  "repository": "git@github.com:Lanseria/i-share-clue.git",
  "author": "lanseria <zhangchao564265135@hotmail.com>",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migration:new": "npm run typeorm migration:create -- -n",
    "migration:gen": "npm run typeorm migration:generate -- -n",
    "migrate": "npm run typeorm migration:run -- -t=false",
    "migration:show": "npm run typeorm migration:show",
    "rollback": "npm run typeorm migration:revert",
    "schema:drop": "npm run typeorm schema:drop",
    "schema:sync": "npm run typeorm schema:sync",
    "typeorm-seeding": "ts-node -r tsconfig-paths/register ./node_modules/typeorm-seeding/dist/cli.js",
    "seed:config": "npm run typeorm-seeding config",
    "seed": "npm run typeorm-seeding seed",
    "seed-user": "yarn typeorm-seeding seed --seed CreateUsersSeed",
    "seed-project": "yarn typeorm-seeding seed --seed CreateYiqingProjectsSeed",
    "trans-project": "ts-node src/helpers/script.ts",
    "upi": "yarn upgrade-interactive"
  },
  "dependencies": {
    "@nestjs/bull": "^0.4.2",
    "@nestjs/common": "^8.2.6",
    "@nestjs/config": "^1.1.6",
    "@nestjs/core": "^8.2.6",
    "@nestjs/jwt": "^7.2.0",
    "@nestjs/passport": "^7.1.6",
    "@nestjs/platform-express": "^8.2.6",
    "@nestjs/swagger": "^5.2.0",
    "@nestjs/typeorm": "^8.0.3",
    "bcryptjs": "^2.4.3",
    "bull": "^4.5.0",
    "class-transformer": "0.4.0",
    "class-validator": "^0.13.2",
    "compression": "^1.7.4",
    "cos-nodejs-sdk-v5": "^2.11.6",
    "dayjs": "^1.10.7",
    "factory.ts": "^0.5.2",
    "faker": "^5.5.3",
    "helmet": "^4.6.0",
    "js-ffmpeg": "^0.0.34",
    "lodash": "^4.17.21",
    "nestjs-minio-client": "^1.1.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.7.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.2",
    "swagger-ui-express": "^4.3.0",
    "tencentcloud-sdk-nodejs": "^4.0.284",
    "typeorm": "^0.2.41",
    "typeorm-seeding": "^1.6.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.2.0",
    "@nestjs/schematics": "^8.0.5",
    "@nestjs/testing": "^8.2.6",
    "@types/bull": "^3.15.7",
    "@types/express": "^4.17.13",
    "@types/geojson": "^7946.0.8",
    "@types/jest": "^27.4.0",
    "@types/lodash": "^4.14.178",
    "@types/node": "^16.11.22",
    "@types/passport-jwt": "^3.0.6",
    "@types/passport-local": "^1.0.34",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.1",
    "jest": "^27.4.7",
    "prettier": "^2.5.1",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.3",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.5.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "engines": {
    "node": ">=14",
    "pnpm": ">=6"
  }
}
